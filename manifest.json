{
  "manifest_version": 2,
  "name": "Disable WebBluetooth",
  "description": "Disables the WebBluetooth API for all websites.",
  "version": "1.3.0",
  "content_scripts": [{
    "matches": ["https://*/*", "http://*/*", "file://*"],
    "match_about_blank": true,
    "all_frames": true,
    "run_at": "document_start",
    "js": ["disable_webbluetooth.js"]
  }],
  "permissions": [
    "tabs",
    "https://*/*",
    "http://*/*",
    "file://*"
  ]
}
